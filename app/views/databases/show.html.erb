    <td>Price: <%= f.text_field :drug_price,:size => 10 %></td>
    <td>Weapons: <%= f.text_field :weapons,:size => 10 %></td>
    <td><%= f.submit "Add drugs" %> </td>
  </tr>    
</table>    
