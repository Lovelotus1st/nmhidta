<%= simple_form_for(@subject) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :database_id %>
    <%= f.input :name %>
    <%= f.input :dob %>
    <%= f.input :ssn %>
    <%= f.input :oln %>
    <%= f.input :gender %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.input :suspect %>
    <%= f.input :misc %>
    <%= f.input :citizenship %>
    <%= f.input :note %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
